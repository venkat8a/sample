pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', url:'https://github.com/venkat9484/Sample.git'
                echo 'clone is success'
            }
        }
        stage('build') {
            steps {
                sh ' mvn clean package'
                echo 'build is success'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
                echo 'Testing is success'
            }
        }
    }
}
